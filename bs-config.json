{
  "notify": false,
  "ghostMode": false,
  "logLevel": "silent",
  "injectChanges": false,
  "files": [
    "./src/*.{js}",
    "./src/assets/scss/*.scss",
    "./src/assets/scss/**/*.scss",
    "./*.{html,css,js,scss}",
    "./dist/*.js",
    "./css/*.css"],
  "watchOptions": { "ignored": "node_modules" },
  "server": {
    "baseDir": "./"
  },
  "open": false
}
